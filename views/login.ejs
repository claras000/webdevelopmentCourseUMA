<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Hardware Webshop Login</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      type="text/css"
      media="screen"
      href="/CSS/main.css"
    />
    <!-- put script here -->
    <script src=""></script>
  </head>
  <body>
    <!-- #login-->
    <section class="login">
      <h1><b>Login Now</b></h1>
      <div class="form">
        <form class="login-form" id="submitForm" action="/" method="post">
          <label for="email">Email:</label>

          <input type="email" id="email" name="email" required /><br /><br />

          <label for="password">Password:</label>

          <input
            type="password"
            id="password"
            name="password"
            required
          /><br /><br />

          <br /><br />

          <button
            type="submit"
            id="submitButton"
            class="button btn btn-primary"
          >
            Login
          </button>
          <br /><br />
          <p>
            You dont have an account ?
            <a href="/register" Link>Register Now</a>
          </p>
          <div id="problemReport"></div>
        </form>
      </div>
    </section>
    <!-- #end login -->
  </body>
</html>
